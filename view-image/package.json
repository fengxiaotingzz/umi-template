{
  "name": "view-image",
  "displayName": "view-image",
  "description": "Display all images under this project, as well as their names and addresses",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.81.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:view-image",
    "onCommand:view-image"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "view-image",
          "title": "view-image",
          "icon": "./icon.png"
        }
      ]
    },
    "views": {
      "view-image": [
        {
          "id": "view-image",
          "name": "view-image"
        }
      ]
    },
    "commands": [
      {
        "command": "view-image",
        "title": "view-image"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "view-image",
          "when": "view === view-image",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/lodash-es": "^4.17.9",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/resemblejs": "^4.1.0",
    "@types/vscode": "^1.81.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "ts-loader": "^9.4.4",
    "typescript": "^5.1.6",
    "webpack": "^5.85.0",
    "webpack-cli": "^5.1.1"
  },
  "dependencies": {
    "jimp": "^0.22.10",
    "lcs-image-diff": "^2.2.0",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "resemblejs": "^5.0.0"
  }
}
